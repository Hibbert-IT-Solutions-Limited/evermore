{
  "name": "evermore",
  "version": "0.0.1",
  "description": "Permanently store your files on the blockchain",
  "main": "index.js",
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "scripts": {
    "test": "jest",
    "service": "node dist/index.js",
    "dev": "babel ./src --out-dir dist && qode dist/index.js",
    "build": "npm-run-all clean transpile &&  webpack --config webpack.config.js",
    "transpile": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "watch": "webpack --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@nodegui/nodegui": "^0.24.0",
    "arweave": "^1.8.0",
    "axios": "^0.19.2",
    "chokidar": "^3.4.0",
    "clean-webpack-plugin": "^3.0.0",
    "config": "^3.3.1",
    "copy-webpack-plugin": "^6.0.3",
    "dialog-node": "git+https://github.com/MikeHibbert/dialog-node.git",
    "file-loader": "^6.0.0",
    "lowdb": "^1.0.0",
    "named-pipes": "0.0.1",
    "native-addon-loader": "^2.0.1",
    "nexe": "^3.3.7",
    "node-notifier": "^7.0.2",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "ps-node": "^0.1.6",
    "rimraf": "^3.0.2",
    "shortid": "^2.2.15",
    "smartweave": "https://arweave.net/AM-u4X2po-3Tx7fma3lRonCfLwrjI42IALwDL_YFXBs",
    "systray": "^1.0.5",
    "taskkill": "^3.1.0",
    "tasklist": "^4.0.1"
  },
  "devDependencies": {
    "@nodegui/packer": "^1.4.1",
    "jest": "^26.4.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "webclient"
    ]
  }
}
